		"@types/classnames": "2.3.4",
		"@types/react-dom": "19.1.9",
		"husky": "9.1.7",
		"lint-staged": "16.2.7",
		"msw": "2.11.6",
		"prettier": "3.6.2",
		"tslint": "6.1.3",
		"tslint-config-prettier": "1.18.0",
		"tslint-react": "5.0.0"
	},
	"msw": {
		"workerDirectory": [
			"build",
			"public"
