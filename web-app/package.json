		"axios": "1.13.2",
		"classnames": "2.5.1",
		"constate": "3.3.3",
		"dayjs": "1.11.19",
		"less": "4.4.1",
		"prop-types": "15.8.1",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"react-toastify": "11.0.5",
