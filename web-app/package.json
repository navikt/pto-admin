{
	"name": "pto-admin",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "vite",
		"dev": "vite",
		"build": "vite build",
		"lint": "tslint ./src/**/*.ts*",
